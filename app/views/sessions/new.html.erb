<h1>Kenny Login's page</h1>

<div class="login">
  <%= form_tag login_path do %>
  <%= label_tag :username, "Username" %>
  <%= text_field_tag :username %><br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Danger Zone" %>
  <% end %>
</div>
<div class="">
  Not a member yet?
</div>
<div class="login">
  <%= form_tag '/users/new', method: 'get' do %>
    <%= label_tag(:name, "Enter your name to get started:") %><br>
    <%= text_field_tag(:name) %>
    <%= submit_tag("Search") %>
  <% end %>
</div>
