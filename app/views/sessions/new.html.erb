<h1>Kenny Login's page</h1>

<div class="login">
  <%= form_tag login_path do %>
  <%= label_tag :username, "Username" %>
  <%= text_field_tag :username %><br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Danger Zone" %>
  <% end %>
</div>
<div class="">
  Not a member yet?
</div>
<div>Enter your name and email to get started:</div>
<div class="login">
  <%= form_tag '/new_user', method: 'post' do %>
    <%= label_tag(:name, "Your Name") %>
    <%= text_field_tag(:name) %><br>
    <%= label_tag(:username, "Your email") %>
    <%= email_field_tag(:username) %>
    <%= submit_tag("Go!") %>
  <% end %>
</div>
