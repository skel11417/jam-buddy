<%= form_for @band do |f|%>
  <%= f.hidden_field :musician_id, value: @band.musician.id %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.label :status %>
  <%= f.select :status,  ["single gigs", "jams", "full-time band", "part-time band", "session work"] %>
  <%= f.label :link %>
  <%= f.text_field :link %>
  <%= f.submit %>
<% end %>
